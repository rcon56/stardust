<article class="post-single">

  <header class="post-title">
    <h1>{{ list.title }}</h1>
  </header>

  <!-- Post Tags -->
   <footer class="post-tags">
  {{ #with list as | list | }}
    {{ #each list.entries as |e| }}
        {{ #if (eq list.kind "tag") }}
        <a href="{{ abs-url list.kind (as-path e.title) }}">
        {{ else }}
        <a class="round-tag" href="{{ abs-url list.kind (as-path e.title) }}">
        {{ /if }}
          {{ e.title }}
          <sup><strong><sup>{{ e.count }}</sup></strong></sup>
        </a>
    {{ /each }}
   {{ /with }}
   </footer>

  <!-- Post Nav -->
  

  <!-- Disqus -->
  
</article>

